<?xml version="1.0" encoding="UTF-8"?>
<config version="10.1.0" urldb="paloaltonetworks">
  <devices>
    <entry name="localhost.localdomain">
      <deviceconfig>
        <system>
          <version>10.1.0</version>
          <hostname>PA-VM</hostname>
        </system>
      </deviceconfig>
      <vsys>
        <entry name="vsys1">
          <address>
            <entry name="Server-1">
              <ip-netmask>192.168.1.100/32</ip-netmask>
            </entry>
            <entry name="Server-2">
              <ip-netmask>192.168.1.101/32</ip-netmask>
            </entry>
            <entry name="Server-3">
              <ip-netmask>192.168.1.102/32</ip-netmask>
            </entry>
            <entry name="Server-4">
              <ip-netmask>192.168.1.103/32</ip-netmask>
            </entry>
            <entry name="Server-5">
              <ip-netmask>192.168.1.104/32</ip-netmask>
            </entry>
            <entry name="Network-DMZ">
              <ip-netmask>10.0.1.0/24</ip-netmask>
            </entry>
            <entry name="Network-LAN">
              <ip-netmask>10.0.2.0/24</ip-netmask>
            </entry>
            <entry name="Network-GUEST">
              <ip-netmask>10.0.3.0/24</ip-netmask>
            </entry>
            <entry name="Unused-Server-1">
              <ip-netmask>192.168.1.200/32</ip-netmask>
            </entry>
            <entry name="Unused-Server-2">
              <ip-netmask>192.168.1.201/32</ip-netmask>
            </entry>
            <entry name="Unused-Server-3">
              <ip-netmask>192.168.1.202/32</ip-netmask>
            </entry>
            <entry name="Unused-Server-4">
              <ip-netmask>192.168.1.203/32</ip-netmask>
            </entry>
            <entry name="Unused-Server-5">
              <ip-netmask>192.168.1.204/32</ip-netmask>
            </entry>
            <entry name="Old-Network-1">
              <ip-netmask>172.16.1.0/24</ip-netmask>
            </entry>
            <entry name="Old-Network-2">
              <ip-netmask>172.16.2.0/24</ip-netmask>
            </entry>
          </address>
          <service>
            <entry name="HTTP-Custom">
              <protocol>
                <tcp>
                  <port>8080</port>
                </tcp>
              </protocol>
            </entry>
            <entry name="HTTPS-Custom">
              <protocol>
                <tcp>
                  <port>8443</port>
                </tcp>
              </protocol>
            </entry>
            <entry name="Database-Port">
              <protocol>
                <tcp>
                  <port>3306</port>
                </tcp>
              </protocol>
            </entry>
            <entry name="Unused-Service-1">
              <protocol>
                <tcp>
                  <port>9999</port>
                </tcp>
              </protocol>
            </entry>
            <entry name="Unused-Service-2">
              <protocol>
                <tcp>
                  <port>9998</port>
                </tcp>
              </protocol>
            </entry>
          </service>
          <!-- NO RULES SECTION - All objects should be unused -->
        </entry>
      </vsys>
    </entry>
  </devices>
</config>
